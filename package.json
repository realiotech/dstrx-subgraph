{
  "name": "erc20-subgraph",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "auth": "graph auth https://api.thegraph.com/deploy/",
    "codegen": "graph codegen",
    "build": "graph build",
    "deploy": "graph deploy --node https://api.thegraph.com/deploy/ --ipfs https://api.thegraph.com/ipfs/ graphprotocol/erc20-subgraph",
    "create-testnet": "graph create --node http://graph-node:8020/ dstrx-testnet-subgraph",
    "create-mainnet": "graph create --node http://graph-node:8020/ dstrx-mainnet-subgraph",
    "remove-local": "graph remove --node http://graph-node:8020/ graphprotocol/erc20-subgraph",
    "deploy-testnet": "graph deploy --node http://graph-node:8020/ --ipfs http://ipfs:5001 dstrx-testnet-subgraph subgraph.testnet.yaml",
    "deploy-mainnet": "graph deploy --node http://graph-node:8020/ --ipfs http://ipfs:5001 dstrx-mainnet-subgraph subgraph.mainnet.yaml" 
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.97.0",
    "@graphprotocol/graph-ts": "0.32.0"
  },
  "packageManager": "yarn@4.8.1+sha512.bc946f2a022d7a1a38adfc15b36a66a3807a67629789496c3714dd1703d2e6c6b1c69ff9ec3b43141ac7a1dd853b7685638eb0074300386a59c18df351ef8ff6",
  "dependencies": {
    "@amxx/graphprotocol-utils": "^1.2.0"
  }
}
